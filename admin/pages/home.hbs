    <script language=javascript>
        function searchuuid(uuid){
            document.getElementById('page_form').value = 'search';
            document.getElementById('uuid_form').value = uuid;
            document.postsearchlink.submit();
        }

        function renderuuid(data, type, row, meta){
            if(type === 'display'){
                data = '<a class="btn btn-green btn-sm" onclick="searchuuid(\''+data+'\')">' + 'Search' + '</a>';
            }
            
            return data;
        }

        function showmodal_response(response, code){
            document.getElementById('responseModal_response').innerHTML = response;            
            $("#responseModal").modal('show');
        }

        function submitPostLink(pagename){
            document.getElementById('pagesubmission').value = pagename;
            document.postlink.submit();
        }

    </script>
